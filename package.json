{
  "name": "neo-pet-shop",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/bs58": "^4.0.1",
    "@types/node": "^12.20.11",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "bs58": "^4.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "npm run buildweb && npm run buildcontract",
    "buildcontract": "cd NeoPetShop && dotnet tool restore && dotnet restore && dotnet build /property:GenerateFullPaths=true /consoleloggerparameters:NoSummary",
    "buildweb": "GENERATE_SOURCEMAP=false react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
